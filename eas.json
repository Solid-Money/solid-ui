{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./ServiceAccountSolid.json",
        "track": "internal"
      },
      "ios": {
        "ascAppId": "6747710981",
        "appleTeamId": "QC9255BHMY",
        "sku": "xyz.solid.ios",
        "name": "Solid - The savings super-app"
      }
    }
  }
}
