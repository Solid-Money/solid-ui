{
    "buildCommand": "npm run export:web",
    "outputDirectory": "dist",
    "devCommand": "expo",
    "cleanUrls": true,
    "framework": null,
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=2592000, must-revalidate"
                }
            ]
        }
    ]
}